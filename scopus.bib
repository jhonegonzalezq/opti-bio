@book{Anich2021,
   abstract = {Many current supply chains have not been designed to take into account the concept of risk, the possibility of applying some risk evaluation methods and the connection of this risk with vulnerability and resilience. For the different topologies of a chain, two objectives must be considered essential when designing a chain or a network supply chain: efficiency (minimization of costs) and effectiveness (maximization of service quality). But facing the risk inherent in the different stages of the chain should also be considered. Moreover, many businesses are working to satisfy the demands of different customer profiles (e.g., industrial and private or on-line and face-to-face). These businesses are managing chains addressed to two or more sets of customers. This is the case of distribution in the pharmaceutical sector, in which there are, broadly speaking, pharmacy and public administration clients. This work aims to further discuss the identification and evaluation of risks for the redesign of a set of chains or a network. This broader view means that risk management is incorporated when designing the flows. Synergistic multi-business chains contribute to better decision-making, as each customer profile is considered both separately and with the rest. A better supply chain performance is expected in this further context of risk management. The risks in multi-business synergistic chains include aspects integrated into different variables referring to the quantitative elements by nature, such as costs and times. They should also reflect qualitative ones, such as the configuration of the chain and its consequences on vulnerability and resilience. Finally, as an example, the proposed methodology is applied to supply chains of the same distribution business in the pharmaceutical sector in Chile. © 2021 by Nova Science Publishers, Inc.},
   author = {Nicolás Anich and Manuel Mateo},
   isbn = {978-168507242-1},
   journal = {Risk Management: An Overview},
   note = {Cited by: 1},
   pages = {83 – 116},
   publisher = {Nova Science Publishers, Inc.},
   title = {Risk Analysis in Synergic Supply Chains},
   url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85131023162&partnerID=40&md5=7a213ed73adf8abc76a140a8d08efb7b},
   year = {2021}
}
@article{Herment2021,
   abstract = {The article analyzes the notion of agricultural specialization looking at the debates on technical innovations and socio-economic development in the countryside between the 18th and the 20th century. It is based on the research literature that has renewed European rural history during the past decades. Three different perspectives are proposed to explore how specialized crops and activities transformed farming and regional organization. Firstly, the pioneering historical work on alternative agricultures by Joan Thirsk serves to compare various definitions and approaches to specialization from the production side. Secondly, the article questions how changes in commercial networks, supply chain and industrial activities can shape the spatial footprint of the specialized rural systems. Thirdly, these systems become the fieldwork for observing the contemporary dynamics of agricultural intensification, technical rationalization, and socio-economic selection. © 2021 Centro de Estudios Historico-Rurales. All rights reserved.},
   author = {Laurent Herment and Niccolò Mignemi},
   doi = {10.24215/15155994e163},
