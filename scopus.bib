   issue = {22},
   journal = {Sustainability (Switzerland)},
   keywords = {Portugal; biomass; decision making; environmental impact; logistics; optimization; supply chain management; sustainability},
   note = {Cited by: 1},
   publisher = {Multidisciplinary Digital Publishing Institute (MDPI)},
   title = {Highlighting Sustainability Criteria in Residual Biomass Supply Chains: A Dynamic Simulation Approach},
   volume = {16},
