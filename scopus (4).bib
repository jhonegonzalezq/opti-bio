	keywords = {Analytic hierarchy process; Climate change; Decision making; Hybrid systems; Hydrogen production; Offshore oil well production; Wind; Wind power; BAD methods; Best-bad method; Case-studies; Energy; Green steel; Hydrogen Energy; Industrial materials; Off-shore wind energy; Renewable energy source; Steel production; Site selection},
	correspondence_address = {A.N. Canat; Yildiz Technical University, Industrial Engineering, Turkey; email: aysenuraycanat@gmail.com},
