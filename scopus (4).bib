@ARTICLE{Nguyen2022,
	author = {Nguyen, Hai-Tra and Safder, Usman and Kim, JeongIn and Heo, SungKu and Yoo, ChangKyoo},
	title = {An adaptive safety-risk mitigation plan at process-level for sustainable production in chemical industries: An integrated fuzzy-HAZOP-best-worst approach},
	year = {2022},
	journal = {Journal of Cleaner Production},
	volume = {339},
	doi = {10.1016/j.jclepro.2022.130780},
	url = {https://scopus.unalproxy.elogim.com/inward/record.uri?eid=2-s2.0-85123921968&doi=10.1016%2fj.jclepro.2022.130780&partnerID=40&md5=7b363229e40c2d6b23c91f508612f4e2},
