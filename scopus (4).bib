@ARTICLE{Srinivasan2023,
	author = {Srinivasan, Sabarathinam and Kumarasamy, Suresh and Andreadakis, Zacharias E. and Lind, Pedro G.},
	title = {Artificial Intelligence and Mathematical Models of Power Grids Driven by Renewable Energy Sources: A Survey},
	year = {2023},
	journal = {Energies},
