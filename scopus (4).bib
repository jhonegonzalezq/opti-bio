@ARTICLE{Wu2025,
	author = {Wu, Hui and Yue, Qiong and Guo, Ping and Xu, Xiaoyu},
	title = {Exploiting the potential of carbon emission reduction in cropping-livestock systems: Managing water-energy-food nexus for sustainable development},
	year = {2025},
	journal = {Applied Energy},
	volume = {377},
