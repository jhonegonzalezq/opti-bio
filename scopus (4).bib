	affiliations = {Graduate Program of Biofuels, Federal University of Uberlândia, Campus Santa Mônica, Av. João Naves de Ávila, 2121, MG, Uberlândia, 38400-902, Brazil; Brazilian Agricultural Research Corporation, Parque Estação Biológica, Av. W3 Norte, S/N, Asa Norte, DF, Brasília, 70770-901, Brazil},
