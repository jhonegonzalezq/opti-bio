@ARTICLE{Hettenhaus2006257,
	author = {Hettenhaus, James},
	title = {Achieving sustainable production of agricultural biomass for biorefinery feedstock},
	year = {2006},
	journal = {Industrial Biotechnology},
	volume = {2},
